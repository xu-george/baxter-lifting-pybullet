<robot name="block">
  <link name="block">

    <contact>
      <friction_anchor/>
      <lateral_friction value="1"/>
      <rolling_friction value="0.0"/>
      <spi_friction value="0.0"/>
      <stiffness value="500"/>
      <damping value="10"/>
    </contact>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0.2"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <visual>
      <origin xyz="0.0125 0.0125 0.0125"/>
      <geometry>
        <box size="0.025 0.025 0.025" />
      </geometry>
      <material name="blue">
        <color rgba="0.5 .5 1 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0125 0.0125 0.0125"/>
      <geometry>
        <box size="0.025 0.025 0.025" />
      </geometry>
    </collision>
  </link>
</robot>

